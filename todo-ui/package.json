{
	"name": "todo-ui",
	"version": "0.4.0",
	"description": "Front end component of the ThymeleafJS TodoMVC example project",
	"author": "Emanuel Rabina <emanuelrabina@gmail.com> (http://www.ultraq.net.nz/)",
	"scripts": {
		"lint": "eslint \"**/*.js\"",
		"build": "mkdirp ../todo-website/source/static && npm run build:images && npm run build:scripts && npm run build:styles",
		"build:images": "ncp source/images ../todo-website/source/static/images",
		"build:scripts": "webpack",
		"build:styles": "postcss source/styles/todo.css --output ../todo-website/source/static/styles/todo.min.css --map",
		"watch": "concurrently --kill-others \"npm run watch:scripts\" \"npm run watch:styles\"",
		"watch:scripts": "npm run build:scripts -- --watch",
		"watch:styles": "npm run build:styles -- --watch"
	},
	"dependencies": {
		"@reduxjs/toolkit": "^1.5.0",
		"@ultraq/dom-utils": "^0.4.0",
		"@ultraq/fetch-utils": "^1.1.0",
		"@ultraq/icu-message-formatter": "^0.9.0",
		"@ultraq/redux-utils": "^0.4.2",
		"director": "^1.2.8",
		"dumb-query-selector": "^3.3.0",
		"keycode": "^2.1.9",
		"redux": "^4.0.1",
		"redux-thunk": "^2.3.0",
		"thymeleaf": "^0.20.0",
		"todomvc-app-css": "^2.3.0",
		"todomvc-common": "^1.0.5"
	},
	"devDependencies": {
		"@babel/core": "^7.10.5",
		"@babel/plugin-proposal-object-rest-spread": "^7.10.4",
		"@babel/plugin-proposal-optional-chaining": "^7.10.4",
		"@babel/preset-env": "^7.10.4",
		"babel-loader": "^8.1.0",
		"concurrently": "^3.6.1",
		"cssnano": "^4.1.10",
		"eslint": "^7.4.0",
		"eslint-config-ultraq": "^2.3.3",
		"eslint-plugin-import": "^2.22.0",
		"mkdirp": "^0.5.1",
		"ncp": "^2.0.0",
		"postcss": "^8.1.10",
		"postcss-cli": "^8.3.0",
		"postcss-import": "^13.0.0",
		"raw-loader": "^0.5.1",
		"source-map-loader": "^0.2.4",
		"uuid": "^3.3.2",
		"webpack": "^5.9.0",
		"webpack-cli": "^4.2.0"
	},
	"private": true
}
